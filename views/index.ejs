              <div class="flex justify-between items-start mb-6">
                <div
                  class="w-16 h-16 rounded-2xl bg-gradient-to-br from-accent-500 to-electric-500 flex items-center justify-center shadow-xl"
                >
                  <i class="fas fa-folder-open text-2xl text-black"></i>
                </div>
                <div class="flex space-x-4">
                  <a
                    href="#"
                    class="text-gray-400 hover:text-accent-400 transition-colors duration-300 hover:scale-110 transform"
                  >
                    <i class="fas fa-external-link-alt text-xl"></i>
                  </a>
                  <a
                    href="#"
                    class="text-gray-400 hover:text-electric-400 transition-colors duration-300 hover:scale-110 transform"
                  >
                    <i class="fab fa-github text-xl"></i>
                  </a>
                </div>
              </div>

              <!-- Project Content -->
              <h3
                class="text-2xl font-bold mb-4 group-hover:text-accent-400 transition-colors duration-300"
              >
                <%= project.name %>
              </h3>
              <p class="text-gray-400 mb-8 text-lg leading-relaxed">
                <%= project.description %>
              </p>

              <!-- Tech Stack -->
              <div class="flex flex-wrap gap-3">
                <% project.tech.forEach(tech => { %>
                <span
                  class="px-4 py-2 text-sm font-mono text-accent-400 black-glass rounded-full hover:bg-accent-500/20 transition-all duration-300 cursor-pointer hover:scale-110 transform border border-white/10"
                >
                  <%= tech %>
                </span>
                <% }) %>
              </div>
            </div>
          </div>
          <% }) %>
        </div>

        <!-- View More Button -->
        <div class="text-center mt-16">
          <a
            href="https://<%= cv.personal.github %>"
            target="_blank"
            class="inline-flex items-center px-12 py-4 bg-gradient-to-r from-accent-500 to-electric-500 rounded-full font-semibold text-black hover:from-accent-400 hover:to-electric-400 transition-all duration-300 transform hover:scale-105 hover:shadow-2xl hover:shadow-accent-500/50 group text-lg"
          >
            View More Projects
            <i
              class="fas fa-arrow-right ml-3 group-hover:translate-x-1 transition-transform"
            ></i>
          </a>
        </div>
      </div>
    </section>
  </body>
</html>
